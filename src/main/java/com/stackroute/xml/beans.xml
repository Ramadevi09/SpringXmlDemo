<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
<!--property based injection-->
    <bean id="actor" class="com.stackroute.domain.Actor">
        <property name="name" value="rama"></property>
        <property name="gender" value="female"></property>
        <property name="age" value="21"></property>
    </bean>
    <!--property based object injection-->
    <bean id="movie" class="com.stackroute.domain.Moive">
        <property name="actor" ref="actor"></property>
    </bean>

</beans>